<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>

<VBox style="-fx-background-color: #18191d;" stylesheets="@style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.FilePickerController">
   <children>
      <Button fx:id="uploadFileBtn" contentDisplay="GRAPHIC_ONLY" mnemonicParsing="false" onAction="#openFileBrowser" prefHeight="100.0" prefWidth="100.0" styleClass="menuButton" text="Button">
         <graphic>
            <ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@images/FileUploadIconWhite.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</VBox>
